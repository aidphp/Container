{
	"name" : "aidphp/di",
	"type" : "library",
	"description" : "dependency injection container",
	"license" : "MIT",
	"keywords" : [
		"container",
		"di",
		"psr-11"
	],
	"authors" : [{
			"name" : "Maxime Bourmaut"
		}
	],
	"require" : {
		"php" : "^7.1.0",
		"psr/container" : "^1.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^7.0"
	},
	"autoload" : {
		"psr-4" : {
			"Aidphp\\Di\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Test\\Aidphp\\Di\\" : "test/"
		},
		"files" : [
			"test/Classes.php"
		]
	}
}